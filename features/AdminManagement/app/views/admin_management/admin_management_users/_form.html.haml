= form_for(@admin_management_user) do |f|
  -if @admin_management_user.errors.any?
    #error_explanation
      %h2
        = pluralize(@admin_management_user.errors.count, "error")
        prohibited this admin_management_user from being saved:

      %ul
        -@admin_management_user.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    %br
    = f.text_field :name
  .field
    = f.label :email
    %br
    = f.text_field :email
  .field
    = f.label :password
    %br
    = f.text_field :password
  .field
    = f.label :phone
    %br
    = f.text_field :phone

  %br
  .actions
    = f.submit :value=>"提交"